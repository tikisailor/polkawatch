{
  "name": "archive",
  "version": "0.1.0",
  "main": "dist/index.js",
  "scripts": {
    "build": "tsc -b",
    "prepack": "rm -rf dist && yarn build",
    "test": "jest",
    "start:docker": "cp -a ../../node_modules/* node_modules && docker-compose up",
    "clean:docker": "docker-compose rm -f && docker volume rm $(basename $(pwd))_pgdata",
    "codegen": "./node_modules/.bin/subql codegen"
  },
  "files": [
    "dist",
    "schema.graphql",
    "project.yaml"
  ],
  "devDependencies": {
    "@polkadot/api": "^7",
    "@subql/cli": "latest",
    "@subql/types": "latest",
    "@types/jest": "^27.4.0",
    "babel-jest": "^27.5.1",
    "jest": "^27.5.1",
    "ts-jest": "^27.0.3",
    "typescript": "^4.1.3"
  },
  "dependencies": {
    "lru-cache": "^7.3.1"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": ".",
    "testEnvironment": "node",
    "testRegex": ".test.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    }
  }
}
