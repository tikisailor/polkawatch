{
  "name": "ddp",
  "version": "0.4.24",
  "description": "",
  "author": "",
  "private": true,
  "license": "UNLICENSED",
  "scripts": {
    "build:workspace": "yarn build",
    "build:workspace:client": "yarn build:client",
    "build:workspace:predocker": " mkdir -p esm_modules/@lqs/types && mkdir -p esm_modules/@lqs/client && cp -a ../lqs-client/* esm_modules/@lqs/client && cp ../lqs/src/queries/query.responses.dtos.ts esm_modules/@lqs/types/index.ts",
    "lock:workspace": "cp ../../yarn.lock .",
    "version:workspace": "yarn version --no-git-tag-version --new-version ",
    "test:workspace": "jest --forceExit",
    "test:workspace:e2e": "echo ...",
    "lint:workspace:test": "yarn lint:test",
    "prebuild": "rimraf dist",
    "build": "nest build",
    "build:openapi:prepare": "yarn test:e2e && mkdir -p ../ddp-client",
    "build:openapi:client": "docker run --rm --user $(id -u):$(id -g) -v ${PWD}/../ddp-client:/local/out -v ${PWD}/ddp-api-spec.json:/local/api-spec.json openapitool",
    "build:client": "yarn build:openapi:prepare && yarn build:openapi:client",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "start": "nest start",
    "start:dev": "nest start --watch",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/ddp/src/main",
    "start:prod-docker": "node dist/src/main",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "lint:test": "eslint \"{src,apps,libs,test}/**/*.ts\"",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./test/jest-e2e.json",
    "ipfs": "gulp",
    "ipfs:clean": "gulp clean"
  },
  "dependencies": {
    "@nestjs/common": "^8.0.0",
    "@nestjs/config": "^2.0.0",
    "@nestjs/core": "^8.0.0",
    "@nestjs/platform-express": "^8.0.0",
    "@nestjs/swagger": "^5.2.0",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.13.2",
    "data-forge": "^1.9.5",
    "joi": "^17.6.0",
    "reflect-metadata": "^0.1.13",
    "rimraf": "^3.0.2",
    "rxjs": "^7.2.0",
    "swagger-ui-express": "^4.3.0"
  },
  "devDependencies": {
    "@nestjs/cli": "^8.0.0",
    "@nestjs/schematics": "^8.0.0",
    "@nestjs/testing": "^8.0.0",
    "@types/express": "^4.17.13",
    "@types/jest": "27.4.0",
    "@types/node": "^16.0.0",
    "@types/supertest": "^2.0.11",
    "@typescript-eslint/eslint-plugin": "^5.0.0",
    "@typescript-eslint/parser": "^5.0.0",
    "axios": "^0.26.1",
    "del": "^6.0.0",
    "eslint": "^8.0.1",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-prettier": "^4.0.0",
    "gulp": "^4.0.2",
    "gulp-typescript": "^6.0.0-alpha.1",
    "ipfs-car": "^0.7.0",
    "jest": "^27.2.5",
    "jest-openapi": "^0.14.2",
    "mustache": "^4.2.0",
    "prettier": "^2.3.2",
    "source-map-support": "^0.5.20",
    "supertest": "^6.1.3",
    "ts-jest": "^27.0.3",
    "ts-loader": "^9.2.3",
    "ts-node": "^10.0.0",
    "tsconfig-paths": "^3.10.1",
    "typescript": "^4.3.5"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "src",
    "testRegex": ".*\\.spec\\.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "collectCoverageFrom": [
      "**/*.(t|j)s"
    ],
    "coverageDirectory": "../coverage",
    "testEnvironment": "node",
    "moduleNameMapper": {
      "^@lqs/client(.*)$": "<rootDir>/../../lqs-client",
      "^@lqs/types(.*)$": "<rootDir>/../../lqs/src/queries/query.responses.dtos"
    }
  }
}
